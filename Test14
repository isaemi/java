import java.util.Scanner;

// area of objects
class Student2
{
	int roll;
	String name;
	
	void getData()
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter Roll and Name : ");
		roll = sc.nextInt();
		name = sc.next();
	}
	void show()
	{
		System.out.println(roll);
		System.out.println(name);
		System.out.println();
	}
}
public class Test14 {

	public static void main(String[] args) {
		
		Student2 []s = new Student2[5];

		for(int i=0; i<3; i++)
		{
			s[i] = new Student2();
			s[i].getData();
		}
		for(int i=0; i<3; i++)
		{
			s[i].show();
		}
	}
}
