//static variable
class Example7
{
	int roll;
	String name;
	static String branch = "CSE";
	
	void getData(int roll, String name)
	{
		this.roll = roll;
		this.name = name;
	}
	void show()
	{
		System.out.println("Roll : " + roll);
		System.out.println("Name : " + name);
		System.out.println("Branch : " + branch);
	}
}
public class Test24 {

	public static void main(String[] args) {
		Example7 s1 = new Example7();
		Example7 s2 = new Example7();
		Example7 s3 = new Example7();
		
		s1.getData(111, "Raj");
		s2.getData(222, "Alok");
		s3.getData(333, "Rita");
		s1.show();
		s2.show();
		s3.show();
	}

}
