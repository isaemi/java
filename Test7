public class Test7 {

	public static void main(String[] args) {
		//loop
		//while loop
			int count = 1;
			while (count <= 5)
			{
				System.out.println(count + " Hello");
				count++;
			}
			
			int i = 1;
			while(i <=100)
			{
				System.out.print(i + " ");
				i++;
			}
			
			int j = 100;
			while (j >=1)
			{
				System.out.print(j + " ");
				j--;
			}
			
			double k = 100;
			while (k>=1)
			{
				System.out.print(k + " ");
				k = k - 0.5;
			}
			
		// do while loop
			int l = 1;
			do			// return at the end
			{
				System.out.println("Hello");
			l++;
			}
			while (l<=5);
	}
}
