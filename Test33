// super - used to call the parent class objects
// this - used to call the current call objects

//accesing the parent class variable
class Employee2
{
	double salary=3000;
}
class Javadev2 extends Employee2
{
	double salary = 5000;
	Javadev2()
	{
		System.out.println("Parent : " + super.salary);
		System.out.println("JavaDev2 : " + salary);
	}
}
public class Test33 {

	public static void main(String[] args) {
		Javadev2 d1 = new Javadev2();
	
	}

}
