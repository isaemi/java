//class, object, and method
class Student1
{
	int roll;
	String name;
	int age;
	
	void getData(int r, String nm, int a)
	{
		roll = r;
		name = nm;
		age = a;
	}
	void show()
	{
		System.out.println(roll);
		System.out.println(name);
		System.out.println(age);
		System.out.println();
	}
}
public class Test13 {

	public static void main(String[] args) {
		
		Student1 s1 = new Student1();
		s1.getData(111, "AAA",20);
		s1.show();
				
		Student1 s2 = new Student1();
		s2.getData(222, "BBB",21);
		s2.show();
	}
}
